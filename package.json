{
	"name": "emoji-log",
	"publisher": "james-formica",
	"displayName": "emoji-log",
	"description": "console.log with a random emoji",
	"repository": {
		"url": "https://github.com/jamesformica/emoji-log"
	},
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"icon": "images/icon.png",
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.emoji-log",
				"title": "Emoji Log ðŸ”¥"
			}
		],
		"configuration": {
			"type": "object",
			"title": "emoji-log",
			"properties": {
				"Emoji Log.emojiList": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "Custom list of emojis to be used by emoji-log. You can also put non-emoji strings here."
				},
				"Emoji Log.logFormat": {
					"type": "string",
					"default": "console.log('$EMOJI', $CURSOR)",
					"description": "The format of the log statement. $EMOJI will be replaced with a random emoji and the cursor will be placed at $CURSOR."
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}